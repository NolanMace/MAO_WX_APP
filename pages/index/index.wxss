.swiper {
    width: 680rpx;
    height: 420rpx;
    margin-left: auto;
    margin-right: auto;
    border-radius: 30rpx;
    overflow: hidden;
    box-shadow: black 4rpx 6rpx 10rpx 4rpx;
}

.swiper_item_image {
    width: 680rpx;
    height: 420rpx;
}

.dots {
    margin-top: 30rpx;
    display: flex;
    justify-content: center;
    align-items: center;
}

.dots view {
    width: 20rpx;
    height: 10rpx;
    margin: 0 6rpx;
    border-radius: 5rpx;
    background-color: #8f8f8e;
}

.dots .active {
    width: 50rpx;
    background-color: #c50102;
    transition: width 0.3s ease;
}

.laxin {
    width: 100%;
    height: 200rpx;
    background-color: cadetblue;
}

.shipment_notice {
    width: 90%;
    margin-top: 20rpx;
    margin-left: auto;
    margin-right: auto;
    border: #c50102 solid 8rpx;
    overflow: hidden;
}

.shipment_notice_fold {
    overflow: hidden;
    transition: height 0.3s ease;
}

.shipment_notice image {
    width: 100%;
    height: 165rpx;
}

.fold_container {
    width: 90%;
    height: 40rpx;
    display: flex;
    margin-top: 10rpx;
    flex-direction: row-reverse;
    align-items: center;
    margin-left: auto;
    margin-right: auto;
}

.fold_container image {
    width: 80rpx;
    height: 40rpx;
}

.navigation {
    height: 140rpx;
    width: 95%;
    margin-left: auto;
    margin-right: auto;
}

.navigation_item_container {
    flex: 1;
}

.top_padding {
    width: 100%;
    position: fixed;
    top: 0;
    left: 0;
}

.navigation-item {
    width: 220rpx;
    height: 110rpx;
    opacity: 0.6;
}

.navigation-item.active {
    opacity: 1;
}

.scroll-view {
    margin-top: 20rpx;
    display: grid;
    grid-template-columns: repeat(2, 1fr);
    justify-items: center;
    align-items: center;
    gap: 25rpx 20rpx;
}

.scroll-item {
    width: 330rpx;
    display: flex;
    flex-direction: column;
    overflow: hidden;
}

.scroll-item-data {
    flex: 1;
    display: flex;
    flex-direction: column;
    justify-content: space-between;
    background-image: url(https://yfsmax.oss-cn-hangzhou.aliyuncs.com/huidi.png);
    background-size: 100% 100%;
    background-repeat: no-repeat;
}

.box_pool_img_container {
    width: 320rpx;
    height: 250rpx;
    position: relative;
}

.box_pool_img_container>image {
    width: 100%;
    height: 100%;
    border: #c50101 solid 6rpx;
}

@keyframes scale {
    0% {
        transform: scale(1);
    }

    50% {
        transform: scale(1.4);
    }

    100% {
        transform: scale(1);
    }
}

.new_label {
    width: 50rpx;
    height: 50rpx;
    position: absolute;
    bottom: 9rpx;
    left: 20rpx;
    overflow: hidden;
    animation: scale 2s ease-in-out infinite;
}

.new_label image {
    width: 100%;
    height: 100%;
    image-rendering: -webkit-optimize-contrast;
}

.pre_sale_label {
    width: 65rpx;
    height: 40rpx;
    position: absolute;
    bottom: 9rpx;
    right: 10rpx;
    background: #fdd000;
    border-radius: 15rpx;
    display: flex;
    justify-content: center;
    align-items: center;
}

#serie-title {
    margin-top: 20rpx;
    margin-left: 6rpx;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
}

#serie-data2 {
    display: flex;
    flex-direction: row;
    align-items: center;
    justify-content: space-between;
    width: 340rpx;
    margin-top: 10rpx;
    margin-left: 5rpx;
    margin-bottom: 5rpx;
}

#serie-data2 image {
    height: 45rpx;
    width: 90rpx;
    margin-right: 22rpx;
    image-rendering: -webkit-optimize-contrast;
}

.tianchong {
    height: 45rpx;
    width: 90rpx;
    margin-right: 22rpx;
}

