.search-container {
    display: flex;
    width: 90%;
    align-items: center;
    padding: 10rpx 20rpx;
    border: black solid 4rpx;
    border-radius: 40rpx;
    overflow: hidden;
}

.search-left {
    flex: 2;
}

.search-left image {
    width: 48rpx;
    height: 48rpx;
    padding: 10rpx 20rpx 10rpx 10rpx;
    vertical-align: middle;
}

.search-left input {
    display: inline-block;
    vertical-align: middle;
    width: calc(100% - 88rpx);
}

@keyframes move {
    0% {
        bottom: 450rpx;
    }

    50% {
        bottom: 470rpx;
    }

    100% {
        bottom: 450rpx;
    }
}

.join_club {
    width: 100rpx;
    height: 130rpx;
    position: fixed;
    right: 10rpx;
    animation: move 2s ease-in-out infinite;
}

.join_club image {
    width: 100rpx;
    height: 130rpx;
    image-rendering: -webkit-optimize-contrast;
}

