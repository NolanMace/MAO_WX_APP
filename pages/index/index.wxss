.windowbars_container {
    width: 700rpx;
    height: 160rpx;
    margin-bottom: 15rpx;
    margin-top: 15rpx;
    overflow-x: scroll;
}

.windowbars_container::-webkit-scrollbar {
    display: none;
    width: 0;
    height: 0;
    color: transparent;
}

.windowbar {
    display: flex;
    flex-direction: column;
    align-items: center;
    height: 150rpx;
    width: 110rpx;
    margin-right: 30rpx;
    flex-shrink: 0;
}

.imageView {
    position: relative;
    height: 110rpx;
    width: 110rpx;
    border-radius: 50%;
    overflow: hidden;
}

.winbar_bg {
    position: absolute;
    bottom: 0;
    height: 110rpx;
    width: 110rpx;
    border-radius: 15rpx;
}

.winbar_pp {
    position: absolute;
    bottom: 0;
    right: 10rpx;
    height: 100rpx;
    width: 60rpx;
}